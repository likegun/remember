{
  "manifest_version": 2,
  "name": "wrq_remember",
  "description": "此插件用于记录上次网页浏览到的位置",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
